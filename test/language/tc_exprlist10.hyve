// Conditional operators and expression lists
bool result: true
void fail(string msg):
	"tc_exprlist10: FAIL: $msg"
	result = false

bool a: false

// Equal operator
a = (10,20,30) == (10,20,30)
if !a: fail("(10,20,30) == (10,20,30)")

a = (10,20,30) == (10,20,31)
if a: fail("(10,20,30) == (10,20,31)")

a = (10,20,30) == (10,21,31)
if a: fail("(10,20,30) == (10,21,31)")

// Not equal operator
a = (10,20,30) != (10,20,30)
if a: fail("(10,20,30) != (10,20,30)")

a = (10,20,30) != (10,20,31)
if !a: fail("(10,20,30) != (10,20,31)")

a = (10,20,30) != (11,20,30)
if !a: fail("(10,20,30) != (11,20,30)")

a = (10,20,30) != (10,21,31)
if !a: fail("(10,20,30) != (10,21,31)")

// Greater than operator
a = (10,20,30) > (10,20,30)
if a: fail("(10,20,30) > (10,20,30)")

a = (10,20,31) > (10,20,30)
if a: fail("(10,20,31) > (10,20,30)")

a = (11,20,30) > (10,20,30)
if a: fail("(11,20,30) > (10,20,30)")

a = (11,21,31) > (10,20,30)
if !a: fail("(11,21,31) > (10,20,30)")

// Lesser than operator
a = (11,21,31) < (10,20,30)
if a: fail("(11,21,31) < (10,20,30)")

a = (11,21,31) < (11,21,31)
if a: fail("(11,21,31) < (11,21,31)")

a = (11,21,31) < (12,22,32)
if !a: fail("(11,21,31) < (12,22,32)")

// Greater than - equal operator
a = (10,20,30) >= (10,20,30)
if !a: fail("(10,20,30) >= (10,20,30)")

a = (10,20,30) >= (10,20,31)
if a: fail("(10,20,30) >= (10,20,31)")

a = (11,21,31) >= (10,20,30)
if !a: fail("(11,21,31) >= (10,20,30)")

// Lesser than - equal operator
a = (11,21,31) <= (10,20,30)
if a: fail("(11,21,31) <= (10,20,30)")

a = (11,21,30) <= (11,21,31)
if !a: fail("(11,21,30) <= (11,21,31)")

a = (11,20,30) <= (11,21,31)
if !a: fail("(11,20,30) <= (11,21,31)")

// And operator
a = (1,1,1) && (1,1,1)
if !a: fail("(1,1,1) && (1,1,1)")

a = (1,1,1) && (1,1,0)
if a: fail("(1,1,1) && (1,1,0)")

a = (1,1,0) && (1,1,0)
if a: fail("(1,1,0) && (1,1,0)")

// Or operator
a = (1,1,1) || (1,1,1)
if !a: fail("(1,1,1) || (1,1,1)")

a = (1,1,1) || (1,1,0)
if !a: fail("(1,1,1) || (1,1,0)")

a = (1,1,0) || (1,1,0)
if a: fail("(1,1,0) || (1,1,0)")

if result: "tc_exprlist10: OK"
