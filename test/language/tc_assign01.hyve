// Test assignments of objects and variables with primitive values
bool result: true

void fail(string msg):
	"tc_assign01: FAIL: $msg"
	result = false

uint32 o, p
var uint32 v
var uint32& v_o

o = 10
p = 15
v = 20
v_o = &o

if o != 10: fail("o != 10 ($o)")
if v != 20: fail("v != 20 ($v)")
if v_o != 10: fail("v_o != 10 ($v_o)")
if v_o != o: fail("v_o != o ($v_o)")
if &v_o != o: fail("v_o != o ($v_o)")

o = v
if o != 20: fail("o != 20 ($o)")
if v_o != 20: fail("o != 20 ($v_o)")

o = 30
v = o
if v != 30: fail("v != 30 ($v)")

if result: "tc_assign01: OK"
