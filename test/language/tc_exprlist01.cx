// Multiple lvalue-assignments
bool result: true
void fail(string msg):
	"tc_exprlist01: FAIL: $msg"
	result = false
	
var uint32 a, b = 10

if a != 10: fail("a != 10 ($a)")
if b != 10: fail("b != 10 ($b)")

a, b = 20
if a != 20: fail("a != 20 ($a)")
if b != 20: fail("b != 20 ($b)")

a, b *= 2
if a != 40: fail("a != 40 ($a)")
if b != 40: fail("b != 40 ($b)")

if result: "tc_exprlist01: OK"
